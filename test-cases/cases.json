[
  {
    "name": "inorder simple",
    "steps": [
      ["insert", 5],
      ["insert", 3],
      ["insert", 7],
      ["insert", 4]
    ],
    "expect": { "type": "inorder", "value": [3, 4, 5, 7] }
  },
  {
    "name": "delete leaf",
    "steps": [
      ["insert", 5],
      ["insert", 3],
      ["insert", 7],
      ["insert", 2],
      ["delete", 2]
    ],
    "expect": { "type": "inorder", "value": [3, 5, 7] }
  },
  {
    "name": "search found",
    "steps": [
      ["insert", 5],
      ["insert", 1],
      ["insert", 9]
    ],
    "expect": { "type": "search", "target": 9, "value": true }
  },
  {
    "name": "search not found",
    "steps": [
      ["insert", 5],
      ["insert", 1],
      ["insert", 9]
    ],
    "expect": { "type": "search", "target": 8, "value": false }
  },
  {
    "name": "preorder check",
    "steps": [
      ["insert", 5],
      ["insert", 3],
      ["insert", 7]
    ],
    "expect": { "type": "preorder", "value": [5, 3, 7] }
  },
  {
    "name": "postorder basic",
    "steps": [
      ["insert", 5],
      ["insert", 3],
      ["insert", 7],
      ["insert", 2],
      ["insert", 4]
    ],
    "expect": { "type": "postorder", "value": [2, 4, 3, 7, 5] }
  },
  {
    "name": "delete one right child",
    "steps": [
      ["insert", 5],
      ["insert", 3],
      ["insert", 7],
      ["insert", 6],
      ["delete", 7]
    ],
    "expect": { "type": "inorder", "value": [3, 5, 6] }
  },
  {
    "name": "delete one left child",
    "steps": [
      ["insert", 5],
      ["insert", 3],
      ["insert", 2],
      ["delete", 3]
    ],
    "expect": { "type": "inorder", "value": [2, 5] }
  },
  {
    "name": "delete two children replace with successor",
    "steps": [
      ["insert", 5],
      ["insert", 3],
      ["insert", 7],
      ["insert", 6],
      ["insert", 8],
      ["delete", 7]
    ],
    "expect": { "type": "inorder", "value": [3, 5, 6, 8] }
  },
  {
    "name": "delete root with two children",
    "steps": [
      ["insert", 5],
      ["insert", 3],
      ["insert", 7],
      ["insert", 6],
      ["insert", 8],
      ["delete", 5]
    ],
    "expect": { "type": "inorder", "value": [3, 6, 7, 8] }
  },
  {
    "name": "delete root with one child",
    "steps": [
      ["insert", 5],
      ["insert", 3],
      ["delete", 5]
    ],
    "expect": { "type": "inorder", "value": [3] }
  },
  {
    "name": "delete to empty",
    "steps": [
      ["insert", 5],
      ["delete", 5]
    ],
    "expect": { "type": "inorder", "value": [] }
  },
  {
    "name": "inorder after mixed inserts",
    "steps": [
      ["insert", 10],
      ["insert", 5],
      ["insert", 15],
      ["insert", 3],
      ["insert", 7],
      ["insert", 13],
      ["insert", 17]
    ],
    "expect": { "type": "inorder", "value": [3, 5, 7, 10, 13, 15, 17] }
  },
  {
    "name": "skewed left preorder",
    "steps": [
      ["insert", 5],
      ["insert", 4],
      ["insert", 3],
      ["insert", 2],
      ["insert", 1]
    ],
    "expect": { "type": "preorder", "value": [5, 4, 3, 2, 1] }
  },
  {
    "name": "skewed right preorder",
    "steps": [
      ["insert", 1],
      ["insert", 2],
      ["insert", 3],
      ["insert", 4],
      ["insert", 5]
    ],
    "expect": { "type": "preorder", "value": [1, 2, 3, 4, 5] }
  },
  {
    "name": "duplicates ignored",
    "steps": [
      ["insert", 5],
      ["insert", 5],
      ["insert", 5],
      ["insert", 3],
      ["insert", 7]
    ],
    "expect": { "type": "inorder", "value": [3, 5, 7] }
  },
  {
    "name": "search in empty tree",
    "steps": [],
    "expect": { "type": "search", "target": 1, "value": false }
  },
  {
    "name": "inorder empty tree",
    "steps": [],
    "expect": { "type": "inorder", "value": [] }
  },
  {
    "name": "multiple deletes chain",
    "steps": [
      ["insert", 10],
      ["insert", 5],
      ["insert", 15],
      ["insert", 3],
      ["insert", 7],
      ["insert", 13],
      ["insert", 17],
      ["delete", 3],
      ["delete", 7],
      ["delete", 13],
      ["delete", 17],
      ["delete", 5],
      ["delete", 15],
      ["delete", 10]
    ],
    "expect": { "type": "inorder", "value": [] }
  }
]
